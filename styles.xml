<?xml version="1.0" encoding="UTF-8"?>
<group>

    <style>
     
        .perfil label {
             align: client;
             horzTextAlign: trailing;
        }
   
        .perfil edit {
             align: right;
             margins: {left=10};
             width: 200;
             transparent: false;
             horzTextAlign: leading;
        }
   
        .titulo label {
             autoSize: true;
             fontSize: 30;
             horzTextAlign: center;
             vertTextAlign: leading;
        }
   
        .atributos edit {
   
        }

        rectangle.fundo {
			align: client;
			color: #40000000;
			xradius: 10;
			yradius: 10;
		}

        flowPart.oneLineTile, flowLayout.oneLineTile {
			stepSizes: {310, 420, 640, 760, 1150};
			minScaledWidth: 300;
		}
		
		flowPart.oneLineTile2, flowLayout.oneLineTile2 {
			stepSizes: {310, 420, 640, 760, 860, 960, 1150, 1200, 1600};
			minScaledWidth: 300;
		}		

        edit.transparente, comboBox.transparente {
			transparent: true;
		}

        textEditor.transparent, textEditor.transparente {
			transparent: true;
		}

        
		edit.infoNoTopo {
			fontSize: 18;
			height: 30;
			transparent: true;
			vertTextAlign: trailing;
			width: 195;
		}

        label.tituloCampo {
			fontSize: 12;
			fontColor: #D0D0D0;
		}

        flowPart.tile, flowLayout.tile {
			stepSizes: {310, 420, 640, 760, 1150};
			minScaledWidth: 300;
		}
        
